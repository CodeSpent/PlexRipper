{
	"name": "plexripper-web-ui",
	"version": "0.11.0",
	"description": "PlexRipper",
	"author": "JasonLandbridge",
	"private": true,
	"scripts": {
		"dev": "nuxi dev",
		"build": "nuxi build",
		"generate": "nuxi generate",
		"preview": "nuxi preview",
		"postinstall": "nuxi prepare",
		"npm-upgrade-packages": "npm-upgrade",
		"cypress:open": "npx cypress open --e2e --browser chromium --env BASE_URL=http://localhost:3030",
		"cypress:run": "npx cypress run",
		"cypress:e2e": "start-server-and-test 'nuxi dev --port 3030 --dotenv .env.cypress' http://localhost:3030 cypress:open",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
		"lint": "npx eslint . --ext .ts,.js,.vue --max-warnings=0",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "npx prettier . --ignore-path ./.prettierignore --check ",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix"
	},
	"dependencies": {
		"@codewithdan/observable-store": "^2.2.15",
		"@microsoft/signalr": "^7.0.5",
		"@vueuse/components": "^10.1.0",
		"@vueuse/rxjs": "^10.1.0",
		"axios": "^1.4.0",
		"axios-observable": "^2.0.0",
		"consola": "^3.1.0",
		"date-fns": "^2.30.0",
		"lodash-es": "^4.17.21",
		"rxjs": "^7.8.1",
		"three": "^0.123.0",
		"vanta": "^0.5.24"
	},
	"devDependencies": {
		"@faker-js/faker": "^7.6.0",
		"@ngneat/falso": "^6.4.0",
		"@nuxt/devtools": "^0.4.5",
		"@nuxtjs/eslint-config-typescript": "^12.0.0",
		"@nuxtjs/i18n": "^8.0.0-beta.10",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@quasar/extras": "^1.16.3",
		"@testing-library/cypress": "^9.0.0",
		"@testing-library/dom": "^9.2.0",
		"@types/lodash-es": "^4.17.7",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.16.3",
		"@typescript-eslint/parser": "^5.59.1",
		"@vue-macros/nuxt": "^1.3.1",
		"@vueuse/core": "^10.1.0",
		"@vueuse/nuxt": "^10.1.0",
		"cypress": "^12.11.0",
		"cypress-signalr-mock": "^1.3.0",
		"cypress-vite": "^1.4.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-cypress": "^2.13.3",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.11.0",
		"factory.ts": "^1.4.0",
		"nuxt": "^3.4.3",
		"nuxt-quasar-ui": "^1.7.3",
		"object-path": "^0.11.8",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"sass-loader": "^13.2.2",
		"start-server-and-test": "^2.0.0",
		"swagger-typescript-api": "^12.0.4",
		"typescript": "^5.0.4",
		"vite-tsconfig-paths": "^4.2.0",
		"vue-tsc": "^1.6.1",
		"why": "^0.6.2"
	}
}
