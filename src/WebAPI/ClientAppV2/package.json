{
	"name": "plexripper-web-ui",
	"version": "0.10.0-RC-2",
	"description": "PlexRipper",
	"author": "JasonLandbridge",
	"private": true,
	"scripts": {
		"dev": "nuxi dev",
		"build": "nuxi build",
		"generate": "nuxi generate",
		"preview": "nuxi preview",
		"postinstall": "nuxi prepare",
		"npm-upgrade-packages": "npm-upgrade",
		"cypress:open": "npx cypress open --component --browser chromium",
		"cypress:run": "npx cypress run",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
		"lint": "npx eslint . --ext .ts,.js,.vue --max-warnings=0",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "npx prettier . --ignore-path ./.prettierignore --check ",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix"
	},
	"dependencies": {
		"@codewithdan/observable-store": "^2.2.15",
		"@microsoft/signalr": "^7.0.4",
		"@vueuse/components": "^9.13.0",
		"@vueuse/rxjs": "^9.13.0",
		"axios": "^1.3.4",
		"axios-observable": "^2.0.0",
		"consola": "^2.15.3",
		"date-fns": "^2.29.3",
		"lodash-es": "^4.17.21",
		"rxjs": "^7.5.5",
		"three": "^0.123.0",
		"vanta": "^0.5.22"
	},
	"devDependencies": {
		"@nuxt/devtools": "^0.3.1",
		"@nuxtjs/eslint-config-typescript": "^12.0.0",
		"@nuxtjs/i18n": "^8.0.0-beta.10",
		"@originjs/vite-plugin-commonjs": "^1.0.3",
		"@quasar/extras": "^1.16.2",
		"@types/node": "^18.15.11",
		"@typescript-eslint/parser": "^5.57.0",
		"@vue-macros/nuxt": "^1.2.7",
		"@vueuse/core": "^9.13.0",
		"@vueuse/nuxt": "^9.13.0",
		"cypress": "^12.9.0",
		"eslint": "^8.37.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.10.0",
		"nuxt": "^3.3.3",
		"nuxt-quasar-ui": "^1.4.0",
		"object-path": "^0.11.8",
		"prettier": "^2.8.7",
		"sass": "^1.60.0",
		"sass-loader": "^13.2.2",
		"swagger-typescript-api": "^12.0.4",
		"typescript": "^5.0.3",
		"vue-tsc": "^1.2.0",
		"why": "^0.6.2"
	}
}
